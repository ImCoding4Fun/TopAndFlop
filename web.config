<?xml version="1.0"?>
<configuration>
   <system.webServer>
      <defaultDocument enabled="true">    <!-- this line enables default documents for a directory -->
         <files>      
          <clear/>     <!-- removes the existing default document list -->                
          <add value="App_Template.html"/>     <!-- foo.htm is now the default document  -->
        	<add value="App.js"/>
        	<add value="App.json"/>
        	<add value="TF_Template.html"/>
        	<add value="TF_Template.css"/>
        	<add value="TF_Template.js"/>
        	<add value="movies.json"/>
        </files>
      </defaultDocument>
   </system.webServer>
</configuration>